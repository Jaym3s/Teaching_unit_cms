%h2= "Upload \"#{@upload.to_param}\""
- content_tag_for :div, @upload do
  %ul
    %li
      %strong Upload file name:
      =h @upload.upload.url
    %li
      %strong Upload content type:
      =h @upload.upload_content_type
    %li
      %strong Upload file size:
      =h @upload.upload_file_size
    %li
      %strong Upload updated at:
      =h @upload.upload_updated_at
%ul
  %li= link_to 'Edit', edit_upload_path(@upload)
  %li= link_to 'Back', uploads_path